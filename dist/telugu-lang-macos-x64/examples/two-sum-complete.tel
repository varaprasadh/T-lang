// Edge case 2: No solution
sankhya demo4b = [1, 2, 3];
printTestCase(4, demo4b, 3, 10);
twoSumBasic(demo4b, 3, 10);

printHeader("Performance Analysis");
analyzePerformance(4);
analyzePerformance(10);
analyzePerformance(100);

